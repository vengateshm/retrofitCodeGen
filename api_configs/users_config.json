{
  "baseUrl": "https://dummyjson.com/",
  "endpoints": [
    {
      "name": "getUsers",
      "path": "/users",
      "method": "GET",
      "responseModel": "UserListResponse"
    },
    {
      "name": "getUserById",
      "path": "/users/{id}",
      "method": "GET",
      "responseModel": "User"
    }
  ],
  "models": {
    "User": {
      "id": "Int",
      "firstName": "String",
      "lastName": "String",
      "age": "Int",
      "gender": "String",
      "email": "String"
    },
    "UserListResponse": {
      "users": "List<User>",
      "total": "Int",
      "skip": "Int",
      "limit": "Int"
    }
  }
}